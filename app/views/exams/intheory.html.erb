<%= render 'dashboards/hNavBar' %>
<%= render 'dashboards/vNavBar' %>
<div class="page-content-wrapper">
        <div class="page-content">
          <div class="page-bar">
            <div class="page-title-breadcrumb">
              <div class=" pull-left">
                <div class="page-title">Export Table</div>
              </div>
              <ol class="breadcrumb page-breadcrumb pull-right">
                <li><i class="fa fa-home"></i>&nbsp;<a class="parent-item" href="index.html">Home</a>&nbsp;<i class="fa fa-angle-right"></i>
                </li>
                <li><a class="parent-item" href="">Data Tables</a>&nbsp;<i class="fa fa-angle-right"></i>
                </li>
                <li class="active">Export Table</li>
              </ol>
            </div>
          </div>
          <div class="row">
            <div class="col-md-12 col-sm-12">
              <div class="card card-box">
                <div class="card-head">
                  <header>Export Table</header>
                </div>
                 <% if @class != ' ' %>
                <div class="card-body " id="bar-parent">
                  <table class="table table-striped table-bordered table-hover table-checkable order-column valign-middle" id="exportTable">
                    <thead>
                      <tr>
                        <th>Rank</th>
                        <th>Name</th>
                        <th>english</th>
                        <th>kiswahili</th>
                        <th>math</th>
                        <th>science</th>
                        <th>social studies</th>
                        <th>CRE</th>
                        <th>Total</th>
                        <th>Year</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                          <td></td>
                          <td><%= @class.firstName+" "+@class.lastName %></td>
                          <td><%= @class.english %></td>
                          <td><%= @class.kiswahili %></td>
                          <td><%= @class.math %></td>
                          <td><%= @class.science %></td>
                          <td><%= @class.social_studies %></td>
                          <td><%= @class.CRE %></td>
                          <td><%= @class.results %></td>
                          <td><%= @formated %></td>
                        </tr>

                    </tbody>
                  </table>
                </div>
                <% else %>
                <p>NOT APPLICABLE</p>
                <% end %>
              </div>
            </div>
          </div>
        </div>
      </div>
<%= render 'dashboards/chat' %>
<%= render 'dashboards/footer' %>
